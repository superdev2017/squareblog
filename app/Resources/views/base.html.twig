<!DOCTYPE html>
<html class="no-js" lang="">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}{{ app_name }}{% endblock %}</title>
    <meta name="description" content="{% block description %}{% endblock %}">
    {% block meta_tags %}{% endblock %}

    <!-- Favicons -->
    <link rel="apple-touch-icon" href="{{ asset('images/misc/favicon/apple-icon-57x57.png') }}">
    <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('images/misc/favicon/apple-icon-57x57.png') }}">
    <link rel="apple-touch-icon" sizes="60x60" href="{{ asset('images/misc/favicon/apple-icon-60x60.png') }}">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('images/misc/favicon/apple-icon-72x72.png') }}">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('images/misc/favicon/apple-icon-76x76.png') }}">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('images/misc/favicon/apple-icon-114x114.png') }}">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('images/misc/favicon/apple-icon-120x120.png') }}">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('images/misc/favicon/apple-icon-144x144.png') }}">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('images/misc/favicon/apple-icon-152x152.png') }}">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('images/misc/favicon/apple-icon-180x180.png') }}">
    <link rel="icon" type="image/png" sizes="192x192"  href="{{ asset('images/misc/favicon/android-icon-192x192.png') }}">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('images/misc/favicon/favicon-32x32.png') }}">
    <link rel="icon" type="image/png" sizes="96x96" href="{{ asset('images/misc/favicon/favicon-96x96.png') }}">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('images/misc/favicon/favicon-16x16.png') }}">
    <link rel="manifest" href="{{ asset('images/misc/favicon/manifest.json') }}">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{{ asset('images/misc/favicon/ms-icon-144x144.png') }}">
    <meta name="theme-color" content="#ffffff">


    <!-- ionicons -->
    <link href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css" rel="stylesheet">

    {% stylesheets '../app/Resources/public/css/bootstrap.min.scss' filter='cssrewrite' %}
    <link href="{{ asset_url }}" rel="stylesheet" crossorigin="anonymous">
    {% endstylesheets %}

    {% stylesheets '../app/Resources/public/css/bootstrap-theme.scss' filter='cssrewrite' %}
    <link href="{{ asset_url }}" rel="stylesheet" crossorigin="anonymous">
    {% endstylesheets %}



    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/featherlight/1.7.0/featherlight.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/featherlight/1.7.0/featherlight.gallery.min.css" />



    <link rel="stylesheet" href="{{ asset('assets/vendor/sweetalert2/dist/sweetalert2.min.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/vendor/flag-icon-css/css/flag-icon.min.css') }}">

    {% stylesheets '../app/Resources/public/scss/theme/theme.scss' filter="compass" %}
    <link href="{{ asset_url }}" rel="stylesheet" crossorigin="anonymous">
    {% endstylesheets %}

    {##}
    {% stylesheets '../app/Resources/public/scss/main.scss' filter='cssrewrite' %}
    <link href="{{ asset_url }}" rel="stylesheet" crossorigin="anonymous">
    {% endstylesheets %}

    {% stylesheets '../app/Resources/public/css/styles-merged.scss' filter='cssrewrite' %}
    <link href="{{ asset_url }}" rel="stylesheet" crossorigin="anonymous">
    {% endstylesheets %}



    {% stylesheets '../app/Resources/public/css/style.scss' filter='cssrewrite' %}
    <link href="{{ asset_url }}" rel="stylesheet" crossorigin="anonymous">
    {% endstylesheets %}


    {% stylesheets '../app/Resources/public/css/theme_style.scss' filter='cssrewrite' %}
    <link href="{{ asset_url }}" rel="stylesheet" crossorigin="anonymous">
    {% endstylesheets %}




    <style type="text/css">
        #pricing {
            background-image: linear-gradient(rgba(0, 0, 0, 0.45), rgba(0, 0, 0, 0.45)), url("{{ asset('images/about/about-img.jpg') }}");
                background-repeat: no-repeat;
                background-size: cover;
        }
        #sign-up {
            background-image: linear-gradient(rgba(0, 0, 0, 0.75), rgba(0, 0, 0, 0.55)), url("{{ asset('images/about/about2.jpg') }}");
            background-size: cover;
            background-repeat: no-repeat;
        }
        #about-img {
            background-image: url("{{ asset('images/about/about1.jpg') }}");
            background-size: cover;
            background-repeat: no-repeat;
            height: 45em;
            background-position-y: -15em;
        }
    </style>


    {% block stylesheets %}{% endblock %}


    {% javascripts '../app/Resources/public/js/jquery-2.2.4.min.js' %}
    <script src="{{ asset_url }}" type="text/javascript" crossorigin="anonymous"></script>
    {% endjavascripts %}

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.inview/1.0.0/jquery.inview.min.js"></script>

    <script src="//ajax.aspnetcdn.com/ajax/jquery.validate/1.11.1/jquery.validate.min.js" type="text/javascript"></script>
    <script src="//ajax.googleapis.com/ajax/libs/webfont/1/webfont.js" type="text/javascript"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js" type="text/javascript"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/featherlight/1.7.0/featherlight.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/featherlight/1.7.0/featherlight.gallery.min.js"></script>


    <script src="{{ asset('assets/vendor/sweetalert2/dist/sweetalert2.min.js') }}"></script>
    <script src="{{ asset('assets/vendor/bootstrap-validator/dist/validator.min.js') }}"></script>

    <!-- Local libs -->
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>

    {% javascripts
    '../app/Resources/public/js/bootstrap.min.js'
    '../app/Resources/public/js/vendor/jquery.stellar.min.js'
    '../app/Resources/public/js/vendor/jquery.waypoints.min.js'
    '../app/Resources/public/js/vendor/simplyCountdown.min.js'
    '../app/Resources/public/js/vendor/jquery.magnific-popup.min.js'
    '../app/Resources/public/js/jquery-skrollr.js'


    %}
    <script src="{{ asset_url }}" type="text/javascript" crossorigin="anonymous"></script>
    {% endjavascripts %}

    {% set currentPath = path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) %}
    {% if currentPath == '/' %}
        {% javascripts
        '../app/Resources/public/js/main.js'
        '../app/Resources/public/js/custom.js'
        %}
        <script src="{{ asset_url }}" type="text/javascript" crossorigin="anonymous"></script>
        {% endjavascripts %}
    {% endif %}
    <!-- Custom JS -->
    {% block javascripts %}{% endblock %}

    <script type="text/javascript">
        var translations = {
            "are.you.sure": "Are you sure?",
            "confirm": "Confirm",
            "no": "No",
            "yes": "Yes",
            "ok": "Ok",
            "your.subscription.has.been.canceled": "Your subscription has been canceled",
            "error": "Error",
            "close": "Close",
            "there.was.an.error.canceling.your.subscription.please.contact.support": "There was an error canceling your subscription, please contat support",
            "success": "Success!",
            "are.you.sure?": "Are you sure?",
            "you.will.get.%product_name%.on.%next_billing_date%.if.you.cancel.you.will.not.be.able.to.recover.your.account": "You will get %product_name% on %next_billing_date%, if you cancel you will not be able to recover your account.",
            "contact.support": "Contact support",
            "disabled": "Disabled",
            "unknown.error.please.try.again": "Unknown error, please try again"
        };
    </script>

